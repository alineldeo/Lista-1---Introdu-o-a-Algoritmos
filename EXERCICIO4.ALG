Algoritmo "exercicio4"
Var
 a, b, vt:inteiro
Inicio
  escreval ("Digite o valor desejado para A:")
  leia (a)
  escreval ("Digite o valor desejado para B:")
  leia (b)
  vt <- a
  a <-b
  b <-vt
  escreval ("Os valores apresentados para A e B, agora são...")
  escreval ("A:",a)
  escreval ("B:",b)
Fimalgoritmo